<h1>Add a collaborator</h1>
<div class="row">
	<div class="col-md-6">
		<%= form_for [@wiki, @collaborator] do |f| %>
			<div class="form-group">
				<%= f.label "Add new collaborators email:" %>
				<%= text_field_tag(:emails, nil, class: "form-control")  %>
			</div>

			<div class="form-group">
				<%= f.submit "Add", class: "btn btn-success" %>
			</div>
			<%= link_to "Back", :back %>
		<% end %>
	</div>
</div>


