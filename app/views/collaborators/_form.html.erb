
<%= form_for [wiki, collaborator] do |f| %>
	<div class="form-group">
		<%= f.label "Add Collaborators" %> (add emails seperated by commas)
		<%= text_field_tag(:collaborators, nil, class: "form-control")  %>
	</div>

	<div class="form-group">
		<%= f.submit "Add", class: "btn btn-success pull-right" %>
	</div>
<% end %>
